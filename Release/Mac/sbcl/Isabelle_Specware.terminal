#!/bin/bash

cd `/usr/bin/dirname $0`
SCRIPTDIR=`pwd`
XEMACS_SPECWARE="$SCRIPTDIR"/XEmacs_Specware

# Basic
PATH=/bin:/usr/local/bin:/usr/bin:/sbin:/usr/sbin:$HOME/bin:${PATH}
# Isabelle, fink, darwinports
PATH=/usr/local/Isabelle/bin:/sw/bin:/opt/local/bin:/opt/local/sbin:$HOME/Isabelle/bin:${PATH}

isabelle emacs -p $XEMACS_SPECWARE $* 
